Program test21
Declare
    Record Stat
        int count;
        int median;
    EndRecord;

    Stat stat;
Begin
    Write "--- Start test21 ---";
    writeln();
    stat := new Stat;
    stat.count := 100;
    stat.median := 47;
    Write "count = ";
    writeint(stat.count);
    writeln();
    Write "median = ";
    writeint(stat.median);
    writeln();
    Write "--- End test21 ---";
    writeln();
End test21.
